
func PrintArray(a->[[int]]; n->int) -> int
	i, j ->int;

	for i = 0 to n-1 do
		for j = 0 to n-1 do
			print a[i][j];
		endfor;
	endfor;

	PrintArray = 0;
endfunc

func main (args->[[char]]) -> int
	a -> [[int]];
	n = 5 -> int;
	i, j -> int;

	a = [[int] n];

	for i = 0 to n-1 do
		a[i] = [int n];

		for j = 0 to n-1 do
			a[i][j] = i + j;
		endfor;
	endfor;

	PrintArray(a, n);

	main = 0;
endfunc
