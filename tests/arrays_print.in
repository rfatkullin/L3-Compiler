
func PrintArray(a->[[int]]; n->int) -> int
	i, j ->int;

	for i = 0 to n-1 do
		for j = 0 to n-1 do
			print(a[i][j]);
		endfor;
	endfor;

	PrintArray = 0;
endfunc

func main (args->[[char]]) -> int
	b -> [[int]];
	n = 5 -> int;
	i, j -> int;

	b = [[int] n];

	for i = 0 to n-1 do
		b[i] = [int n];

		for j = 0 to n-1 do
			b[i][j] = i + j;
		endfor;

	endfor;

	PrintArray(b, n);

	main = 0;
endfunc
